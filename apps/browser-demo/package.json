{
	"name": "@cyrus/browser-demo",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"description": "Browser-based interactive demo emulator for Cyrus CLI",
	"scripts": {
		"dev": "node dist/server.js",
		"start": "node dist/server.js",
		"start:emulator": "node dist/server.js --emulator",
		"start:real": "node dist/server.js",
		"typecheck": "tsc --noEmit",
		"build": "tsc"
	},
	"dependencies": {
		"@cyrus/agent-runners": "workspace:*",
		"cyrus-interfaces": "workspace:*",
		"cyrus-orchestrator": "workspace:*",
		"cyrus-storage": "workspace:*",
		"express": "^4.18.2",
		"ws": "^8.14.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/node": "^20.10.4",
		"@types/ws": "^8.5.10",
		"typescript": "^5.3.3"
	}
}
