{
	"name": "cyrus-interfaces",
	"version": "0.1.0",
	"description": "TypeScript interface definitions for Cyrus I/O abstractions",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./test/contracts": {
			"import": "./test/contracts/IUserInterface.contract.test.js",
			"types": "./test/contracts/IUserInterface.contract.test.d.ts"
		}
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "vitest",
		"test:run": "vitest run",
		"typecheck": "tsc --noEmit"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"cyrus",
		"interfaces",
		"io",
		"abstractions"
	],
	"author": "Ceedar",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.0.0",
		"typescript": "^5.3.3",
		"vitest": "^3.0.0"
	}
}
