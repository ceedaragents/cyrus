{
	"name": "cyrus-storage",
	"version": "0.1.0",
	"description": "File-based storage implementation for Cyrus session management",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
