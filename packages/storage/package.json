	},
	"devDependencies": {
		"@types/node": "^20.0.0",
		"@vitest/coverage-v8": "^1.6.1",
		"typescript": "^5.3.3",
		"vitest": "^1.1.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
