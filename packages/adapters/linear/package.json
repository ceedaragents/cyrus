{
	"name": "cyrus-adapter-linear",
	"version": "0.1.0",
	"description": "Linear-specific implementation of IUserInterface for Cyrus",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"typecheck": "tsc --noEmit",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"dependencies": {
		"@linear/sdk": "^60.0.0",
		"cyrus-interfaces": "workspace:*",
		"cyrus-linear-webhook-client": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "^20.0.0",
		"typescript": "^5.3.3",
		"vitest": "^3.0.0",
		"@vitest/coverage-v8": "^3.0.0"
	},
	"keywords": [
		"cyrus",
		"linear",
		"adapter",
		"io-interface"
	],
	"author": "Ceedar",
	"license": "ISC"
}
